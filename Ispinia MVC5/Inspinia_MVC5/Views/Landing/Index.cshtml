    <div id="inSlider" class="carousel carousel-fade" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#inSlider" data-slide-to="0" class="active"></li>
            <li data-target="#inSlider" data-slide-to="1"></li>
            <li data-target="#inSlider" data-slide-to="2"></li>
            <li data-target="#inSlider" data-slide-to="3"></li>
            <li data-target="#inSlider" data-slide-to="4"></li>
        </ol>
        <div class="carousel-inner" role="listbox">

            @*Primo Item del Carosel*@
            <div class="item active">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>
                            Benvenuti<br />
                            a tutti<br />
                            Gli internauti
                        </h1>
                        <p>
                            Accomodatevi,<br />
                            centellinate una tisana<br />
                            e scoprite chi sono...
                        </p>
                    </div>
                    <div class="carousel-image wow zoomIn">
                        <img src="~/Images/landing/1.jpg" alt="me" />
                    </div>
                </div>
                <!-- Set background for slide in css -->
                <div class="header-back one"></div>
            </div>

            @*Secondo Item del Carosel*@
            <div class="item">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>
                            Cosmologo<br />
                            ed astrofilo<br />
                            per formazione
                        </h1>
                        <p>
                            Gallerie fotografiche,<br />
                            le migliori immaigni di Hubble<br />
                            e tante altre curiosità
                        </p>
                    </div>
                    <div class="carousel-image wow zoomIn">
                        <img src="~/Images/landing/Cosmosmall.jpg" alt="me" />
                    </div>
                </div>
                <!-- Set background for slide in css -->
                <div class="header-back two"></div>
            </div>

            @*Terzo Item del Carosel*@
            <div class="item">
                <div class="container">
                    <div class="carousel-caption">
                        <h1>
                            Software engineer<br />
                            ed It Architect <br />
                            per professione
                        </h1>
                        <p>
                            Le mie esperienze professionali:<br />
                            Fortran, C++, Action Script, Php, Java, VB.NET e C#<br />
                            per giungere alla progettazione e gestione di architetture<br />
                            Cloud Azure ed On-Premises nel mondo Microsoft
                        </p>
                    </div>
                    <div class="carousel-image wow zoomIn">
                        <img src="~/Images/landing/azure-postersmall.jpg" alt="me" />
                    </div>
                </div>
                <!-- Set background for slide in css -->
                <div class="header-back two"></div>
            </div>

            @*Quarto Item del Carosel*@
